[OSEv3:children]
masters
nodes
glusterfs
glusterfs_registry

[OSEv3:vars]
ansible_ssh_user=root
openshift_deployment_type=origin

[masters]
master  master=True

[nodes]
node0  openshift_schedulable=True
node1  openshift_schedulable=True
node2  openshift_schedulable=True
node3  openshift_schedulable=True
node4  openshift_schedulable=True
node5  openshift_schedulable=True

[glusterfs]
node0  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
node1  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
node2  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'

[glusterfs_registry]
node3  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
node4  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
node5  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
