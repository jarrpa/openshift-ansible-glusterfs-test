[OSEv3:children]
masters
nodes
glusterfs_registry

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin

[masters]
master  master=True

[nodes]
node0  openshift_schedulable=True
node1  openshift_schedulable=True
node2  openshift_schedulable=True

[glusterfs_registry]
node0  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
node1  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
node2  glusterfs_devices='[ "/dev/sdb", "/dev/sdc", "/dev/sdd" ]'
